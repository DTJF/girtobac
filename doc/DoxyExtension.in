@INCLUDE = @CMAKE_CURRENT_SOURCE_DIR@/Doxyfile

OUTPUT_DIRECTORY=@CMAKE_CURRENT_BINARY_DIR@
FILTER_SOURCE_PATTERNS = *.bas=fb-doc \
                         *.bi=fb-doc
FILTER_SOURCE_FILES    = YES
FILTER_PATTERNS        = *.bas=fb-doc \
                         *.bi=fb-doc

PROJECT_NAME=@PROJ_NAME@
PROJECT_BRIEF="@PROJ_DESC@"
PROJECT_NUMBER=@PROJ_VERS@
ALIASES = "Email=@PROJ_MAIL@" \
          "Proj=*@PROJ_NAME@*" \
          "Year=@PROJ_YEAR@" \
          "Webs=@PROJ_WEBS@" \
          "GMP=[GMarkupParser](https://developer.gnome.org/glib/stable/glib-Simple-XML-Subset-Parser.html)"

